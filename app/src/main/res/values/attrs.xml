<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomRoundedButton">
        <attr name="textButton" format="string"/>
        <attr name="src" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomButton">
        <attr name="textBtn" format="string"/>
        <attr name="isRounded" format="boolean"/>
        <attr name="enable" format="boolean"/>
        <attr name="robotoLight" format="boolean"/>
        <attr name="transparentDisabled" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CustomToolbar">
        <attr name="title" format="string"/>
        <attr name="backSrc" format="reference"/>
        <attr name="pic1Src" format="reference"/>
    </declare-styleable>

    <declare-styleable name="CustomRadioGroup">
        <attr name="initPosition" format="integer"/>
        <attr name="textFirst" format="string"/>
        <attr name="textSecond" format="string"/>
        <attr name="textThird" format="string"/>
    </declare-styleable>
</resources>